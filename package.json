{
  "name": "smoothy",
  "version": "1.4.7",
  "description": "discord music bot",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc -p typescript/tsconfig.json",
    "dev": "concurrently \"npx tsc -p typescript/tsconfig-dev.json\" \"nodemon ./build/main.js\"",
    "start": "node ./build/main.js",
    "linux": "mono ./RunSmoothy-Linux/Program.exe",
    "windows": "./RunSmoothy/Run Rmoothy.exe",
    "postinstall": "patch-package"
  },
  "author": "Aaron Graybill",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "@discordjs/voice": "^0.14.0",
    "config": "^3.3.9",
    "discord.js": "^14.7.1",
    "ffmpeg-static": "^5.0.2",
    "figlet": "^1.5.2",
    "flatted": "^3.2.5",
    "miniget": "^4.2.2",
    "node-abort-controller": "^3.0.1",
    "opusscript": "^0.0.8",
    "play-dl": "^1.9.5",
    "prism-media": "^1.3.2",
    "sax": "^1.2.4",
    "tweetnacl": "^1.0.3",
    "yt-search": "^2.10.3",
    "ytdl-core": "^4.11.4",
    "ytdl-core-discord": "^1.3.1",
    "ytpl": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "patch-package": "^6.5.1"
  }
}
